<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/timeResultBlock_year_backgroundCover"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/timeResultBlock_year"
        app:layout_constraintEnd_toEndOf="parent"/>

    <include
        layout="@layout/artifact_single_time_result"
        android:id="@+id/timeResultBlock_year"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/timeResultBlock_marginStart"
        app:layout_constraintEnd_toStartOf="@id/timeResultBlock_year_resizeAnchor" />

    <View
        android:id="@+id/timeResultBlock_year_resizeAnchor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/timeResultBlock_month"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/timeResultBlock_month_backgroundCover"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/timeResultBlock_month"
        app:layout_constraintEnd_toEndOf="parent"/>

    <include
        layout="@layout/artifact_single_time_result"
        android:id="@+id/timeResultBlock_month"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/timeResultBlock_marginStart"
        app:layout_constraintEnd_toStartOf="@id/timeResultBlock_month_resizeAnchor" />

    <View
        android:id="@+id/timeResultBlock_month_resizeAnchor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/timeResultBlock_week"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/timeResultBlock_week_backgroundCover"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
