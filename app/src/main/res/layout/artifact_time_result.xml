        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/timeResultBlock_minute"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/timeResultBlock_minute_backgroundCover"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/timeResultBlock_minute"
        app:layout_constraintEnd_toEndOf="parent"/>

    <include
        layout="@layout/artifact_single_time_result"
        android:id="@+id/timeResultBlock_minute"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/timeResultBlock_marginStart"
        app:layout_constraintEnd_toStartOf="@id/timeResultBlock_minute_resizeAnchor" />

    <View
        android:id="@+id/timeResultBlock_minute_resizeAnchor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
