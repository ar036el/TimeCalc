    <include
        layout="@layout/artifact_single_time_result"
        android:id="@+id/timeResultBlock_millisecond"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/timeResultBlock_millisecond_resizeAnchor" />

    <View
        android:id="@+id/timeResultBlock_millisecond_resizeAnchor"
        android:layout_width="10dp"
        android:translationX="10dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/timeResultBlock_end_backgroundCover"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/timeResultBlock_millisecond"
        app:layout_constraintEnd_toEndOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>